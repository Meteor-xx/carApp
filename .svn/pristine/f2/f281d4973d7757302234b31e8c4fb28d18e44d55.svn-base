<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_white"
    android:padding="@dimen/px10dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/px20sp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/px60sp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/car_choose_state"
                android:layout_width="@dimen/px30sp"
                android:layout_height="@dimen/px30sp"
                android:src="@mipmap/car_offline"
                android:layout_gravity="center_vertical"/>
            <TextView
                android:id="@+id/car_choose_item"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/px36sp"
                android:layout_marginLeft="@dimen/px30sp"
                android:textColor="@color/color_999999"
                android:text="云驾专用车"/>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>